{
  "name": "dagre-d3",
  "version": "0.0.1",
  "description": "d3 dagre layout",
  "main": "lib/dagre-d3.cjs.js",
  "jsnext:main": "lib/dagre-d3.es.js",
  "module": "lib/dagre-d3.es.js",
  "author": "Richard Roncancio",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf lib dist && mkdirp lib dist",
    "rollup-cjs": "rollup -c config/rollup.config.cjs.js",
    "rollup-es": "rollup -c config/rollup.config.es.js",
    "rollup-iife": "rollup -c config/rollup.config.iife.js",
    "rollup": "npm-run-all --parallel rollup-cjs rollup-es rollup-iife",
    "prepublish": "npm run build",
    "minify": "uglifyjs -mc < dist/dagre-d3.js > dist/dagre-d3.min.js",
    "build": "npm-run-all clean rollup minify"
  },
  "devDependencies": {
    "babel-preset-es2015-rollup": "^3.0.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.0.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.41.1",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-cleanup": "^1.0.0",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-filesize": "^1.0.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "uglifyjs": "^2.4.10"
  },
  "dependencies": {
    "d3": "^4.4.1",
    "dagre": "^0.7.4",
    "graphlib": "^2.1.1",
    "lodash": "^4.17.4"
  }
}
